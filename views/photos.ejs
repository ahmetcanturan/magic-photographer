<%- include("partials/header.ejs")%>



   <%- include("partials/menu.ejs")%>

      <!-- gallery -->
      <div class="gallery">
         <div class="container_with">
            <div class="row">
               <div class="col-md-12">
                  <div class="titlepage text_align_center">
                     <img src="/images/logo1.png" alt="Italian Trulli">
                     <span></span>
                     <h2>Photos</h2>
                  </div>
               </div>
            </div>
            <div class="tz-gallery">
               <div class="row">
                  <% for (photo of photos){%>
                     <div class="col-lg-4 col-md-6 ma_bottom30">
                        <div class="lightbox">
                           <img src=" <%= photo.image %>" alt="<%= photo.name %>">
                           <div class="view_main">
                              <div class="pose">
                                 <a class="read_more" href="/photos/<%=photo.id%>"><img src="images/ga.png"
                                       alt="#" /></a>
                                 <h3 style="padding-top:10px; padding-bottom:10px">
                                    <%= photo.name %>
                                 </h3>
                                 <p style="padding-top:10px;">
                                    <%= photo.description.substring(0,50)+"..." %>
                                 </p>
                              </div>
                           </div>
                        </div>
                     </div>
                     <% }%>
               </div>
            </div>
         </div>
      </div>
      <!-- end gallery -->
      <%- include("partials/footer.ejs")%>